{
  "name": "youtube",
  "title": "YouTube",
  "author": "thislooksfun",
  "home": "https://www.youtube.com",
  "share": "https://www.youtube.com/watch?v=[id]",
  "icon": "??",
  "manifest_version": 2,
  "scripts": {
    "prep": "src/prep.js",
    "play": "src/play.js",
    "search": "src/search.js",
    "info": "src/info.js"
  },
  "extension": {
    "matches": ["*://www.youtube.com/watch?v=*"],
    "js": [
      {
        "path": "scripts/extension/setup.js",
        "web_accessible": true
      }
    ],
    "run_at": "document_end"
  },
  "dependencies": {
    "iso8601-duration": "^1.1.1",
    "youtube-api": "^2.0.9"
  }
}